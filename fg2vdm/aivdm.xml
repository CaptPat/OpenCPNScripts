<?xml version="1.0"?>

<PropertyList>
 <comment>
  <![CDATA[
    This is the protocol definition for aivdm - meant to be further converted to AIS !AIVDM format by fg2vdm.py

    It can be used with any scenario, but is limited to 3 carriers, 9 escorts, 9 ships and 9 AI aircraft

    Usage:
    # fgfs --generic=socket,out,1,<host>,5501,udp,aivdm

    Check data with:
    #  netcat -u -l -p 5501
  ]]>
 </comment>

 <generic>

  <output>
   <line_separator>newline</line_separator>
   <var_separator>,</var_separator>

   <chunk>
    <name>latitude</name>
    <type>float</type>
    <format>LAT=%03.5f</format>
    <node>/ai/models/carrier/position/latitude-deg</node>
   </chunk>

   <chunk>
    <name>longitude</name>
    <type>float</type>
    <format>LON=%03.5f</format>
    <node>/ai/models/carrier/position/longitude-deg</node>
   </chunk>

   <chunk>
    <name>altitude</name>
    <type>float</type>
    <format>ALT=%03.5f</format>
    <node>/ai/models/carrier/position/altitude-ft</node>
   </chunk>

   <chunk>
    <name>orientation</name>
    <type>float</type>
    <format>HDG=%03.5f</format>
    <node>/ai/models/carrier/orientation/true-heading-deg</node>
   </chunk>

   <chunk>
    <name>speed</name>
    <type>float</type>
    <format>SOG=%03.5f</format>
    <node>/ai/models/carrier/velocities/speed-kts</node>
   </chunk>

   <chunk>
    <name>sign</name>
    <type>string</type>
    <format>SIGN=%s</format>
    <node>/ai/models/carrier/sign</node>
   </chunk>

   <chunk>
    <name>callsign</name>
    <type>string</type>
    <format>CALLSIGN=%s</format>
    <node>/ai/models/carrier/callsign</node>
   </chunk>

   <chunk>
    <name>name</name>
    <type>string</type>
    <format>NAME=%s,CARRIER\n</format>
    <node>/ai/models/carrier/name</node>
   </chunk>

